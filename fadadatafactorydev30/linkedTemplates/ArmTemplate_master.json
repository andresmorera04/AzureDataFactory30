{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "fadadatafactorydev30"
		},
		"ls_azuresql_configuracion_parametros_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "secrazuresqlintegrator"
		},
		"ls_datalake_30_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().pr_url_datalake}"
		},
		"ls_keyvault_30_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().pr_url_keyvault}"
		},
		"ls_restapi_fakestoreapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://fakestoreapi.com"
		},
		"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "devvertix.ddns.net,5722"
		},
		"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "BikeStores"
		},
		"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "integrator"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "fadadatafactorydev30_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_keyvault_30_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_30_properties_typeProperties_baseUrl')]"
					},
					"ls_restapi_fakestoreapi_properties_typeProperties_url": {
						"value": "[parameters('ls_restapi_fakestoreapi_properties_typeProperties_url')]"
					},
					"ls_azuresql_configuracion_parametros_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('ls_azuresql_configuracion_parametros_properties_typeProperties_connectionString_secretName')]"
					},
					"ls_datalake_30_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_30_properties_typeProperties_url')]"
					},
					"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_server')]"
					},
					"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_database')]"
					},
					"ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_devvertixddnsnet_bikestores_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "fadadatafactorydev30_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'fadadatafactorydev30_ArmTemplate_0')]"
			]
		},
		{
			"name": "fadadatafactorydev30_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'fadadatafactorydev30_ArmTemplate_1')]"
			]
		}
	]
}